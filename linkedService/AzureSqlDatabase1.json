{
	"name": "AzureSqlDatabase1",
	"properties": {
		"parameters": {
			"p_sqlserver": {
				"type": "String",
				"defaultValue": "eddemodb.database.windows.net"
			},
			"p_sqldb": {
				"type": "String",
				"defaultValue": "AdventureworksLT"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"server": "@{linkedService().p_sqlserver}",
			"database": "@{linkedService().p_sqldb}",
			"encrypt": "optional",
			"trustServerCertificate": false,
			"authenticationType": "SystemAssignedManagedIdentity"
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}